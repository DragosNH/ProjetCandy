{
  "expo": {
    "name": "CandyCrushProject",
    "slug": "CandyCrushProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/title_img.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/title_img.png",
      "resizeMode": "contain",
      "backgroundColor": "#003367"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/title_img.png",
        "backgroundColor": "#003367"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
